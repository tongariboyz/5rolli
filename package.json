{
  "name": "5rolli",
  "version": "0.0.0",
  "author": "tongariboyz",
  "description": "SHINCHOKU Viewer",
  "bugs": {
    "url": "https://github.com/tongariboyz/5rolli/issues"
  },
  "dependencies": {
    "dacho": "^1.0.0",
    "moment": "^2.10.6",
    "nbem": "^0.2.0",
    "normalize.css": "^3.0.3",
    "os-homedir": "^1.0.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.1",
    "redux": "^3.0.4",
    "redux-actions": "^0.9.0",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.2",
    "superagent": "^1.5.0",
    "zenigame": "0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-plugin-espower": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chokidar-cli": "^1.2.0",
    "clear-require": "^1.0.1",
    "codecov.io": "^0.1.6",
    "cpx": "^1.2.1",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.36.1",
    "esdoc": "^0.4.3",
    "eslint": "^1.10.1",
    "eslint-config-kanmu": "^9.0.0",
    "eslint-plugin-react": "^3.10.0",
    "exorcist": "^0.4.0",
    "flow-bin": "^0.20.0",
    "gh-pages": "^0.8.0",
    "isparta": "^4.0.0",
    "jade": "^1.11.0",
    "mocha": "^2.3.4",
    "node-inspector": "^0.12.3",
    "npm-run-all": "^1.3.4",
    "power-assert": "^1.2.0",
    "proxyquire": "^1.7.3",
    "stylus": "^0.53.0",
    "watchify": "^3.6.1"
  },
  "homepage": "https://github.com/tongariboyz/5rolli#readme",
  "keywords": [
    "electron",
    "trello"
  ],
  "license": "MIT",
  "main": "main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tongariboyz/5rolli.git"
  },
  "scripts": {
    "app": "electron --debug=5858 dist/main.js",
    "build": "npm-run-all --parallel build:*",
    "build:css": "stylus -c src/styles/base.styl --include-css -I node_modules -o dist/bundle.css -m --sourcemap-root stylus",
    "build:fonts": "cpx \"src/fonts/**/{*.eot,*.svg,*.ttf,*.woff}\" dist/fonts",
    "build:images": "cpx \"src/images/**/{*.svg,*.gif,*.png,*.jpg}\" dist/images",
    "build:jade": "jade src --out dist",
    "build:js-main": "browserify -t babelify src/js/main/main.js --im --no-detect-globals --node -d | exorcist ./dist/main.js.map > ./dist/main.js",
    "build:js-renderer": "browserify -t babelify src/js/renderer/app.js -d | exorcist ./dist/bundle.js.map > ./dist/bundle.js",
    "codecov": "cat coverage/lcov.info | codecov",
    "deploy": "npm-run-all doc gh-pages",
    "doc": "esdoc -c esdoc.json",
    "gh-pages": "gh-pages -d doc",
    "lint": "eslint --cache {src/js,test}",
    "prebuild": "rm -rf {dist,build} && mkdir -p {dist,build}",
    "predoc": "rm -rf doc",
    "pretest": "rm -rf coverage",
    "prewatch": "mkdir -p {dist,build}",
    "start": "npm run watch",
    "stop": "npm-run-all stop:*",
    "stop:flow": "flow stop",
    "test": "npm-run-all lint test:coverage",
    "test:coverage": "babel-node $(npm bin)/isparta cover --report text --report html --report lcovonly _mocha -- test/**/*spec.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "stylus -c -w src/styles/base.styl --include-css -I node_modules -o dist/bundle.css -m --sourcemap-root ./stylus",
    "watch:flow": "flow & chokidar src/js/ -c flow",
    "watch:fonts": "cpx \"src/fonts/**/{*.eot,*.svg,*.ttf,*.woff}\" dist/fonts --watch",
    "watch:images": "cpx \"src/images/**/{*.svg,*.gif,*.png,*.jpg}\" dist/images --watch",
    "watch:jade": "jade src --watch --out dist",
    "watch:js-main": "watchify -v -t babelify ./src/js/main/main.js --im --no-detect-globals --node -o \"exorcist ./dist/main.js.map > ./dist/main.js\" -d",
    "watch:js-renderer": "watchify -v -t babelify src/js/renderer/app.js -o \"exorcist dist/bundle.js.map > ./dist/bundle.js\" -d",
    "watch:lint": "chokidar src/js test -c 'npm run -s lint'",
    "watch:server": "node-inspector",
    "watch:test": "mocha --watch test/**/*spec.js"
  }
}
